  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment.block", "comment.line"],
      "settings": {
        "foreground": "#7A7E85"
      }
    },
    {
      "name": "Documentation",
      "scope": ["comment.block.documentation"],
      "settings": {
        "foreground": "#5F826B"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#A9B7C6"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "keyword.operator.new",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#CF8E6D"
      }
    },
    {
      "name": "Operator, Misc, Arrow Function",
      "scope": [
        "punctuation.accessor",
        "punctuation.separator",
        "punctuation.terminator",
        "meta.template.expression",
        "storage.type.function.arrow",
        "keyword.operator",
        "constant.other.color",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#BCBEC4"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        // "meta.method.declaration meta.var.expr variable.other.property",
        "meta.object.member meta.function-call entity.name.function",
        "meta.object.member meta.object-literal.key entity.name.function"
      ],
      "settings": {
        "foreground": "#56A8F5"
      }
    },
    // {
    //   "name": "Block Level Variables",
    //   "scope": ["meta.block variable.other"],
    //   "settings": {
    //     "foreground": "#f07178"
    //   }
    // },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#2AACB8"
      }
    },
    {
      "name": "Parameter",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#BCBEC4"
      }
    },
    {
      "name": "Constant, Tag Attribute, Embedded",
      "scope": [
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#CF8E6D"
      }
    },
    {
      "name": "String, Symbols, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#6AAB73"
      }
    },
    {
      "name": "Primitives",
      "scope": ["support.type.primitive"],
      "settings": {
        "foreground": "#CF8E6D"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types",
        "entity.other.inherited-class",
        "new.expr entity.name.function"
      ],
      "settings": {
        "foreground": "#BCBEC4"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#CF8E6D"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Properties",
      "scope": [
        "variable.other.object.property",
        "variable.other.constant.property",
        "variable.other.property",
        "support.variable.property",
        "meta.method.declaration meta.object.member variable.other.property",
        "meta.object.member meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#C77DBB"
      }
    },
    {
      "name": "Decorators",
      "scope": ["meta.decorator", "meta.decorator entity.name.function"],
      "settings": {
        "foreground": "#B3AE60"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
